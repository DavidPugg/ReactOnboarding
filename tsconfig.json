{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "preserve",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@css/*": [
        "css/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "resolveJsonModule": true
  },
  "include": [
    "src",
    "pages",
    "Pages/pages/index.tsx",
    "Pages/PopularPage.tsx",
    "Pages/SearchPage.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
